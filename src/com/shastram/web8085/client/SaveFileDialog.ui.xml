<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:my="urn:import:com.shastram.web8085.client">
  <ui:style field="css" src="Style.css" />
  
  <g:VerticalPanel addStyleNames="{css.saveFileDialog}" horizontalAlignment="ALIGN_CENTER">
    <g:HorizontalPanel>
      <g:Label addStyleNames="{css.dialogLabelWithMargin}">Filename:</g:Label>
      <g:TextBox addStyleNames="{css.dialogLabel}" ui:field="fileName"></g:TextBox>
    </g:HorizontalPanel>
    <g:Label addStyleNames="{css.dialogLabel}" ui:field="status"></g:Label>
    <g:HorizontalPanel>
      <g:Button addStyleNames="{css.saveFileDialogButtons}" ui:field="saveButton">Save</g:Button>
      <g:Button addStyleNames="{css.saveFileDialogButtons}" ui:field="dismissButton">Dismiss</g:Button>
    </g:HorizontalPanel>
  </g:VerticalPanel>
</ui:UiBinder>