<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .important {
    	font-weight: bold;
    }
  </ui:style>
  <g:DockLayoutPanel  unit='EM' height="8000px" width="100%">
    <g:north size='10'>
        <g:VerticalPanel></g:VerticalPanel>
    </g:north>
    <g:east size='5'><g:VerticalPanel></g:VerticalPanel></g:east>
    <g:west size='5'><g:VerticalPanel></g:VerticalPanel></g:west>
    <g:south size='10'><g:VerticalPanel></g:VerticalPanel></g:south>
  
    <g:center>
        <g:VerticalPanel>
            <g:HorizontalPanel spacing="10">
                <g:RichTextArea ui:field="sourceCode" width="450px" height="300px"></g:RichTextArea>
                <g:ListBox ui:field="exeWindow" width="100px" visibleItemCount="20"></g:ListBox>
            </g:HorizontalPanel>
            <g:TextBox ui:field="errorWindow" width="300px"></g:TextBox>
            <g:Button ui:field="compile">Compile</g:Button>
        </g:VerticalPanel>
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder> 