<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:my="urn:import:com.shastram.web8085.client">
  <ui:style field="css" src="Style.css" />
  <g:HorizontalPanel addStyleNames="{css.listFileSingleItem}">
      <g:VerticalPanel>
          <g:Label ui:field="fileName" addStyleNames="{css.listFileSingleItemFileName}"></g:Label>
          <g:Label ui:field="lastModified" addStyleNames="{css.listFileSingleItemDateUpdate}"></g:Label>
      </g:VerticalPanel>
      <g:Button ui:field="openFileButton">Open</g:Button>
      <g:Button ui:field="deleteFileButton">Delete</g:Button>
  </g:HorizontalPanel>
</ui:UiBinder>